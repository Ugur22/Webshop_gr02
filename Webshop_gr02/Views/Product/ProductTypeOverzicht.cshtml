@model List<Webshop_gr02.Models.ProductType>

@{
    ViewBag.Title = "ProductTypeOverzicht";
}



<div class="content_wrap"> 
   <h2>Overzicht Producten</h2>
<h2></h2>
    <table id="categorie">
        <thead>
            <tr>
               
                <th>Naam</th> 
                <th>InkoopPrijs</th>
                <th>VerkoopPrijs</th>
                <th>Omschrijving</th>
                <th>ImagePath</th>
                 <th>Zichtbaar</th>
                 <th>Aanbieding</th>
            </tr>
        </thead>
        <tbody>
            @for(int i = 0; i < Model.Count; i++)
            { 
                <tr>
                    
                    <td>@Model[i].Naam</td>
                     <td>@Model[i].InkoopPrijs</td>
                     <td>@Model[i].VerkoopPrijs</td>
                     <td>@Model[i].Omschrijving</td>
                     <td>@Model[i].ImagePath</td>
                     <td>@Model[i].Zichtbaar</td>
                     <td>@Model[i].Aanbieding</td>

                </tr>
            }
        </tbody>
        </table>

   
</div>
